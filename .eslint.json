{
  "env": {
      "browser": true,
      "node": true,
      "es6": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
      "ecmaFeatures": {
          "experimentalObjectRestSpread": true,
          "jsx": true,
          "modules": true
      },
      "sourceType": "module"
  },
  "plugins": [
      "react"
      
  ],
  "rules": {
      "no-console": "off",
      "react/jsx-uses-react": "error",
      "react/jsx-uses-vars": "error",
      "indent": [2, 2, {"SwitchCase": 1}],
      "linebreak-style": [
          "error",
          "unix"
      ],
      "semi": [
          "error",
          "always"
      ]
  }
}